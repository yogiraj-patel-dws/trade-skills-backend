{
  "name": "trade-skills-backend",
  "version": "1.0.0",
  "description": "Backend API for Trade Skills Platform - Skill exchange with credit-based economy",
  "main": "src/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.js",
    "dev:ts": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prod": "NODE_ENV=production PORT=8000 node dist/app.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:payment": "NODE_ENV=test jest tests/payment.test.js",
    "test:meet": "NODE_ENV=test jest tests/meet.test.js",
    "test:integration": "NODE_ENV=test jest tests/integration.test.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset",
    "db:test:setup": "NODE_ENV=test prisma migrate reset --force && NODE_ENV=test prisma db push"
  },
  "dependencies": {
    "@100mslive/server-sdk": "^0.3.0",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "prisma": "^5.7.0",
    "razorpay": "^2.9.2",
    "redis": "^5.10.0",
    "stripe": "^14.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "skill-exchange",
    "backend",
    "api",
    "credit-system",
    "sessions",
    "payments"
  ],
  "author": "Trade Skills Team",
  "license": "MIT"
}
